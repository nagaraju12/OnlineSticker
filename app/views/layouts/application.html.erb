<%= link_to 'Stickers', "#" %>
  <%#= link_to "list of stickers",  listing_stickers_path, :remote => true %>

     </div>

                     
<div id= "sign_in" >
<% if current_user.present? %>
<%#= link_to 'Account', edit_user_registration_path %>
