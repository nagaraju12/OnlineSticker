<p id="notice"><%= notice %></p>
<h1>Listing Stickers</h1>

<table class="table table-bordered table-striped">
  <thead>
    <tr>
    
      <th>Name</th>
      <th>Decription</th>
      <th>Price</th> 
      <th>Image</th> 
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stickers.each do |sticker| %>
      <tr>
    
        <td><%= sticker.name %></td>
        <td><%= sticker.description %></td>
        <td><%= sticker.price %></td>
        <td><% sticker.images.each do |image| %></td>
<td><%= image_tag image.image %></td>
<% end %>
    <% end %>
  </tbody>
</table>

<br>



