<p id="notice"><%= notice %></p>
<h1>Listing Stickers</h1>

<table >
  <thead>
    <tr>
    
      <th>Name</th>
      <th>Decription</th>
      <th>Price</th> 
      <th>Images</th> 
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stickers.each do |sticker| %>
      <tr>
    
        <td><%= sticker.name %></td>
        <td><%= sticker.description %></td>
        <td><%= sticker.price %></td>
      <% sticker.images.each do |image| %>
<td><%= image_tag image.image %></td>
<% end %>
    <% end %>
  </tbody>
</table>

<br>



