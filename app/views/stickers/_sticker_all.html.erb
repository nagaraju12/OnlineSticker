<p id="notice"><%= notice %></p>
<h1><%= link_to "+New Sticker", new_sticker_path %></h1>
<div class="text-right">
<%= button_to "Back" , dashboards_index_path, :method => "get" %>
</div>

<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Price</th> 
      <th>Images</th>
   <th>Caption part</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @stickers.each do |sticker| %>
      <tr>
        <td><%= sticker.name %></td>
        <td><%= sticker.description %></td>
        <td><%= sticker.price %></td>
      <td><% sticker.images.each do |image| %>
<%= link_to  image_tag(image.image.url, :height => 50, :width =>80 ), sticker_path(sticker.id) %>
<% end %></td>

      </tr>
      
    <% end %>
  </tbody>
</table>
 
<br>
<style>
   div.text-right{
     float: right;
}
  </style>


